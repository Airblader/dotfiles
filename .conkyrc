out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0
text_buffer_size 1024

update_interval 1.0
total_run_times 0

short_units yes
if_up_strictness address
use_spacer left
override_utf8_locale yes
cpu_avg_samples 2

TEXT
,[ \
  { \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "active-window" \
  } \
  ,{ \
    "full_text": "${exec $HOME/scripts/active-window}  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "active-window" \
  } \
${if_running pidgin}
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "pidgin" \
  } \
  ,{ \
    "full_text": "${execi 5 $HOME/scripts/pidgin-count}  ", \
    "color": "\#FFFFFF", \
    "name": "pidgin" \
  } \
${endif}
${if_match "${exec $HOME/scripts/volume-control.py status}" == "on"}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "${execi 5 $HOME/scripts/volume-color.py}", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "toggle-volume" \
  } \
  ,{ \
    "full_text": "${exec $HOME/scripts/volume-control.py read}%  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "${execi 5 $HOME/scripts/volume-color.py}", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "min_width": 40, \
    "align": "right", \
    "name": "volume" \
  } \
${else}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#B33A3A", \
    "background": "\#232D34", \
    "border": "\#B33A3A", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "toggle-volume" \
  } \
  ,{ \
    "full_text": "muted  ", \
    "color": "\#FFEEEE", \
    "background": "\#232D34", \
    "border": "\#B33A3A", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "toggle-volume" \
  } \
${endif}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "${execi 30 $HOME/scripts/battery-color.py}", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "battery" \
  } \
  ,{ \
    "full_text": "${battery_percent}%  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "${execi 30 $HOME/scripts/battery-color.py}", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "min_width": 40, \
    "align": "right", \
    "name": "battery" \
  } \
${if_up wlan0}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "network" \
  } \
  ,{ \
    "full_text": "${wireless_bitrate wlan0}  ${wireless_essid wlan0}  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "network" \
  } \
${endif}
${if_up eth0}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "network" \
  } \
  ,{ \
    "full_text": "${downspeed eth0}  ${addr eth0}  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "network" \
  } \
${endif}
  ,{ "full_text": "    ", "color": "\#E5E511", "separator": false, "separator_block_width": 0, "name": "separator" } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "calendar" \
  } \
  ,{ \
    "full_text": "${time %a, %d. %B}  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "calendar" \
  } \
  ,{ \
    "full_text": "   ", \
    "color": "\#9FBC00", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "clock" \
  } \
  ,{ \
    "full_text": "${time %H:%M:%S}  ", \
    "color": "\#FFFFFF", \
    "background": "\#232D34", \
    "border": "\#9FBC00", \
    "border_bottom": false, \
    "border_left": false, \
    "border_right": false, \
    "separator": false, \
    "separator_block_width": 0, \
    "name": "clock" \
  } \
]
