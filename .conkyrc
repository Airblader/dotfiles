out_to_x no
own_window no
out_to_console yes
background no
max_text_width 0

update_interval 1.0
total_run_times 0

short_units yes
if_up_strictness address
use_spacer left
override_utf8_locale yes
cpu_avg_samples 2

TEXT
[

# Volume
  {
#    "full_text": " ${exec amixer get Master | grep -o '\[.*%\]' | paste -s -d'-'} ",
    "full_text": " ${exec amixer get Master | grep -o '\[.*%\]' | head -n1} ",
    "color": "\#A3ADB4"
  }

# Battery
  ,{
    "full_text": " [${battery_percent}%] ",
    "color": "\#A3ADB4"
  }

# Wifi
${if_up wlan0}
  ,{
    "full_text": " [${wireless_link_qual_perc wlan0}%] ${wireless_essid wlan0} ",
    "color": "\#A3ADB4"
  }
${endif}

# Ethernet
${if_up eth0}
  ,{
    "full_text": "  ${addr eth0} ",
    "color": "\#A3ADB4"
  }
${endif}

# Time
  ,{
    "full_text": " [${time %a %d %b} ${time %H:%M:%S}] ",
    "color": "\#FFFFFF"
  }
],
