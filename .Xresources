URxvt.geometry: 140x30
URxvt.transparent: True
URxvt.shading: 20
URxvt.fading: 30

URxvt.thickness: 1
URxvt.scrollBar: off

URxvt.borderLess: False

URxvt.urgentOnBell: True
URxvt.visualBell: False

URxvt.perl-ext-common: default,clipboard,selection-to-clipboard

