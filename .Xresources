!Green
*color2:  #B7CE42
*color10: #BDE077

!Yellow
*color3:  #FEA63C
*color11: #FFE863

!Blue
*color4:  #66AABB
*color12: #AACCBB

!Magenta
*color5:  #F26B9E
*color13: #FF5879

!Cyan
*color6:  #72AD8C
*color14: #9FE3BC

!White
*color7:  #DDEEDD
*color15: #FFFFFF
